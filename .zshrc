export ZSH="/home/birgizz/.oh-my-zsh"
ZSH_THEME="robbyrussell"
plugins=(git)
source $ZSH/oh-my-zsh.sh
alias zshconfig="mate ~/.zshrc"
alias ohmyzsh="mate ~/.oh-my-zsh"
alias idep="cd $HOME/Documents/projects/dewire"
alias c='clear'
alias prog="cd $HOME/Documents/projects"
alias ee="exit"
alias zcon="nvim $HOME/.zshrc"
alias config='/usr/bin/git --git-dir=$HOME/dotfiles/ --work-tree=$HOME'
alias nvimcon="nvim $HOME/.config/nvim/init.vim"
